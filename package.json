{
  "name": "@softwarity/ttaaii-provider",
  "version": "1.0.2",
  "description": "WMO TTAAII code completion provider - UI agnostic",
  "type": "module",
  "main": "./dist/ttaaii-provider.cjs.js",
  "module": "./dist/ttaaii-provider.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ttaaii-provider.es.js",
      "require": "./dist/ttaaii-provider.cjs.js"
    },
    "./tables.en.json": "./dist/tables.en.json",
    "./tables.fr.json": "./dist/tables.fr.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "start": "ng serve demo",
    "build:demo": "ng build demo",
    "watch:demo": "ng build demo --watch"
  },
  "keywords": [
    "wmo",
    "ttaaii",
    "meteorology",
    "gts",
    "completion",
    "autocomplete"
  ],
  "author": "Softwarity",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^19.2.0",
    "@angular/build": "^19.2.0",
    "@angular/cli": "^19.2.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@softwarity/interactive-code": "^1.0.6",
    "@types/node": "^22.0.0",
    "prismjs": "^1.30.0",
    "@types/prismjs": "^1.26.5",
    "@vitest/coverage-v8": "^4.0.18",
    "rxjs": "^7.8.0",
    "tslib": "^2.8.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "zone.js": "^0.15.0"
  }
}
